<div class="container">
    <div class="row">
        <table class="table">
          <thead>
            <tr>
              <th scope="col">#</th>
              <th scope="col">Model name</th>
              <th scope="col">price</th>
              <th scope="col">Image</th>
            </tr>
          </thead>
          <tbody>
              {{#each product}}
            <tr>
                
              <th scope="row">*</th>
              <td>Galaxy {{this.name}}</td>
              <td>{{this.price}}</td>
              <td><img src="{{this.image}}" width="65rem" height="auto"></td>
            </tr>
            {{/each}}
          </tbody>
        </table>
    </div>
</div>